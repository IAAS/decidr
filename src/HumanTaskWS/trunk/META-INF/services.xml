<?xml version="1.0" encoding="UTF-8"?>
<serviceGroup>
	<service name="HumanTask">
		<messageReceivers>
			<messageReceiver mep="http://www.w3.org/ns/wsdl/in-out"
				class="org.apache.axis2.rpc.receivers.RPCMessageReceiver" />
		</messageReceivers>
		<parameter name="ServiceClass">de.decidr.webservices.humantask.HumanTask
		</parameter>
		<parameter name="useOriginalwsdl">true</parameter>
		<parameter name="modifyUserWSDLPortAddress">true</parameter>
		<operation name="removeTask" mep="http://www.w3.org/ns/wsdl/in-out"
			namespace="http://decidr.de/webservices/HumanTask">
			<actionMapping>http://decidr.de/webservices/HumanTask/removeTask
			</actionMapping>
			<outputActionMapping>
				http://decidr.de/webservices/HumanTask/removeTaskResponse
			</outputActionMapping>
			<faultActionMapping faultName="transactionException">
				http://decidr.de/webservices/HumanTask/HumanTaskPT/removeTask/Fault/transactionException
			</faultActionMapping>
		</operation>
		<operation name="taskCompleted" mep="http://www.w3.org/ns/wsdl/in-out"
			namespace="http://decidr.de/webservices/HumanTask">
			<actionMapping>http://decidr.de/webservices/HumanTask/taskCompleted
			</actionMapping>
			<outputActionMapping>
				http://decidr.de/webservices/HumanTask/taskCompletedResponse
			</outputActionMapping>
			<faultActionMapping faultName="transactionException">
				http://decidr.de/webservices/HumanTask/HumanTaskPT/taskCompleted/Fault/transactionException
			</faultActionMapping>
			<faultActionMapping faultName="reportingException">
				http://decidr.de/webservices/HumanTask/HumanTaskPT/taskCompleted/Fault/reportingException
			</faultActionMapping>
		</operation>
		<operation name="createTask" mep="http://www.w3.org/ns/wsdl/in-out"
			namespace="http://decidr.de/webservices/HumanTask">
			<actionMapping>http://decidr.de/webservices/HumanTask/createTask
			</actionMapping>
			<outputActionMapping>
				http://decidr.de/webservices/HumanTask/createTaskResponse
			</outputActionMapping>
			<faultActionMapping faultName="transactionException">
				http://decidr.de/webservices/HumanTask/HumanTaskPT/createTask/Fault/transactionException
			</faultActionMapping>
		</operation>
		<operation name="removeTasks" mep="http://www.w3.org/ns/wsdl/in-out"
			namespace="http://decidr.de/webservices/HumanTask">
			<actionMapping>http://decidr.de/webservices/HumanTask/removeTasks
			</actionMapping>
			<outputActionMapping>
				http://decidr.de/webservices/HumanTask/removeTasksResponse
			</outputActionMapping>
			<faultActionMapping faultName="transactionException">
				http://decidr.de/webservices/HumanTask/HumanTaskPT/removeTasks/Fault/transactionException
			</faultActionMapping>
		</operation>
	</service>
</serviceGroup>
