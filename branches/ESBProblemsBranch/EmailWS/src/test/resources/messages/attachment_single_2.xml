	<!-- This test makes sure that an attachment is not attached twice. -->
<soap:Envelope xmlns:soap="http://schemas.xmlsoap.org/soap/envelope/"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ema="http://decidr.de/webservices/Email"
	xmlns:dec="http://decidr.de/schema/DecidrTypes" xmlns:dec1="http://decidr.de/schema/DecidrWSTypes"
>
	<soap:Header />
	<soap:Body>
		<ema:sendEmail>
			<ema:to>
				<dec:emailUser>
					<dec:email>decidr.iaas@googlemail.com
					</dec:email>
				</dec:emailUser>
			</ema:to>
			<ema:cc xsi:nil="true" />
			<ema:bcc xsi:nil="true" />
			<ema:fromName>idiot</ema:fromName>
			<ema:fromAddress>root@localhost</ema:fromAddress>
			<ema:subject>attachment single 2</ema:subject>
			<ema:headers xsi:nil="true" />
			<ema:bodyTXT>ddddddddassdf</ema:bodyTXT>
			<ema:bodyHTML xsi:nil="true" />
			<!-- Please make sure it exists -->
			<ema:attachments>
				<dec1:id>10</dec1:id>
				<dec1:id>10</dec1:id>
			</ema:attachments>
		</ema:sendEmail>
	</soap:Body>
</soap:Envelope>